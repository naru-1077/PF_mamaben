<div class="container">
  <div class="row">
    <div class="col-md-12">

    <h2 class="title">会員一覧(全<%= @users.count %>件)</h2>
    <table class="table table-hover table-inverse table-borderless" >
      <thead>
        <tr style="background-color: #ffe6ca">
          <th>会員ID</th>
          <th>アカウント名</th>
          <th>メールアドレス</th>
          <th>会員ステータス</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= link_to user.name, admins_user_path(user) %></td>
          <td><%= user.email %></td>
          <td>
            <% if user.withdraw_status == false %>
              有効
            <% else %>
              退会
            <% end %>
          </td>
        </tr>
        <% end %>
      </tbody>
    </table>
    <%= paginate @users %>